<?xml version="1.0" encoding="UTF-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_focused="true">
        <shape >
            <gradient
                android:startColor="#66CD00"
                android:endColor="#458B74"
                android:angle="90"
                />
            <corners android:radius="10dp"
                />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape >
            <gradient
                android:startColor="#66CD00"
                android:endColor="#458B74"
                android:angle="90"
                />
            <corners android:radius="10dp"
                />
        </shape>
    </item>
    <item >
        <shape >
            <gradient
                android:startColor="@color/bg_intraware_color"
                android:endColor="@color/bg_intraware_container_style"
                android:angle="90"
                />
            <stroke
                android:color="@color/bg_intraware_container_style"
                android:width="0.5dp"
                />
            <corners android:radius="10dp"
                />
        </shape>
    </item>
</selector>