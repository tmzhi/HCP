<?xml version="1.0" encoding="UTF-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_focused="true">
        <shape >
            <gradient 
                android:startColor="@color/bg_intraware_color"
                android:endColor="@color/bg_intraware_color"
                />
            <corners android:radius="5dp"
                />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape >
            <gradient 
                android:startColor="@color/bg_intraware_color"
                android:endColor="@color/bg_intraware_color"
                />
            <corners android:radius="5dp"
                />
        </shape>
    </item>
    <item >
        <shape>
        <corners android:radius="5dp"
            />
        <stroke
            android:color="@color/bg_intraware_color"
            android:width="1dp"
            />
        </shape>
    </item>
</selector>