<?xml version="1.0" encoding="utf-8"?>
    <LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingLeft="8dp"
        android:paddingRight="8dp"
        android:paddingBottom="16dp"
        android:paddingTop="16dp"
        >
    <TextView
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:id="@+id/tv_item_isp_wip_query_produce_organization"
        android:layout_gravity="center_vertical"
        android:textSize="14sp"
        android:gravity="center_horizontal"
        android:textColor="@color/black"
        />
    <TextView
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:id="@+id/tv_item_isp_wip_query_produce_wip"
        android:layout_gravity="center_vertical"
        android:textSize="14sp"
        android:gravity="center_horizontal"
        android:textColor="@color/black"
        />
    <TextView
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:textSize="14sp"
        android:id="@+id/tv_item_isp_wip_query_produce_project_no"
        android:layout_gravity="center_vertical"
        android:gravity="center_horizontal"
        android:textColor="@color/black"
        />
    <TextView
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:textSize="14sp"
        android:id="@+id/tv_item_isp_wip_query_so"
        android:layout_gravity="center_vertical"
        android:gravity="center_horizontal"
        android:textColor="@color/black"
        />
    <TextView
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:textSize="14sp"
        android:id="@+id/tv_item_isp_wip_query_required_organization"
        android:layout_gravity="center_vertical"
        android:gravity="center_horizontal"
        android:textColor="@color/black"
        />
    <TextView
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:textSize="14sp"
        android:id="@+id/tv_item_isp_wip_query_required_wip"
        android:layout_gravity="center_vertical"
        android:gravity="center_horizontal"
        android:textColor="@color/black"
        />
    <TextView
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:textSize="14sp"
        android:id="@+id/tv_item_isp_wip_query_required_project_no"
        android:layout_gravity="center_vertical"
        android:gravity="center_horizontal"
        android:textColor="@color/black"
        />
    <TextView
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:textSize="14sp"
        android:id="@+id/tv_item_isp_wip_query_pr"
        android:layout_gravity="center_vertical"
        android:gravity="center_horizontal"
        android:textColor="@color/black"
        />
    </LinearLayout>